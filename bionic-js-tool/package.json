{
  "name": "bionicjs",
  "version": "0.0.0",
  "description": "bionic.js let you use JavaScript classes from other programming languages without using any framework and without writing a single line of bridging code.",
  "keywords": ["code generation", "java", "android", "swift", "ios", "multiplatform", "bundle"],
  "homepage": "https://github.com/Forge-Srl/bionic-js",
  "license": "MIT",
  "author": "Forge S.r.l.",
  "maintainers": [
    {
      "name": "Marco Vanetti",
      "email": "marco.vanetti@gmail.com"
    },
    {
      "name": "Tommaso Bossi",
      "email": "tommaso.bossi@forge.srl"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Forge-Srl/bionic-js.git"
  },
  "scripts": {
    "compile-parser": "node node_modules/pegjs/bin/pegjs --allowed-start-rules \"TagLine, Tags, BionicTag, DescriptionTag\" -o parser/annotation/Parser.js parser/annotation/pegjsSrc/Parser.pegjs",
    "lint": "./node_modules/.bin/eslint .",
    "test": "jest",
    "release": "np --no-yarn --no-publish --no-release-draft"
  },
  "jest-comment": "the transform:{} config is to disable transpillation and avoid problems while debugging async code",
  "jest": {
    "transform": {},
    "testTimeout": 600000
  },
  "bin": {
    "bionicjs": "cli/cli.js"
  },
  "files": [
    "/cli",
    "/filesystem",
    "/generation",
    "/parser",
    "/schema",
    "/Bjs.js"
  ],
  "dependencies": {
    "@babel/parser": "^7.12.7",
    "commander": "^6.2.1",
    "fast-glob": "^3.2.4",
    "graceful-fs": "^4.2.4",
    "ignore": "^5.1.8",
    "memfs": "^3.2.0",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "touch": "^3.1.0",
    "uuid": "^8.3.1",
    "webpack": "^5.9.0",
    "xcode": "^3.0.1"
  },
  "devDependencies": {
    "copy-dir": "^1.3.0",
    "eslint": "^7.20.0",
    "jest-cli": "^26.6.3",
    "pegjs": "^0.10.0",
    "np": "^7.4.0"
  }
}
